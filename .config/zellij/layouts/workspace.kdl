layout {
    default_tab_template {
        children
        pane size=1 borderless=true {
            plugin location="compact-bar"
        }
    }
    tab name="main" {
        pane command="hx" args="." close_on_exit=true
    }
    tab name="run" {
        pane
    }
    tab name="git" {
        pane command="lazygit"
    }
}
